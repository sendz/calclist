<interface>
  <!-- interface-name item.ui -->
  <requires lib="gtk" version="4.12"/>
  <object class="GtkBox" id="item">
    <property name="spacing">4</property>
    <property name="valign">baseline-center</property>
    <child>
      <object class="GtkEntry" id="item_name">
        <property name="hexpand">True</property>
        <property name="hexpand-set">True</property>
      </object>
    </child>
    <child>
      <object class="GtkEntry" id="item_operator">
        <property name="max-width-chars">1</property>
        <property name="width-chars">1</property>
      </object>
    </child>
    <child>
      <object class="GtkEntry" id="item_amount"/>
    </child>
  </object>
</interface>
